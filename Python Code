      },
      "source": [
        "LDA first"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "m2n3JPtFQ_ZD"
      },
      "source": [
        "%%capture\n",
        "from sklearn.feature_extraction.text import CountVectorizer\n",
        "#cv = CountVectorizer(max_df=0.95, min_df=2, stop_words=stop_words)\n",
        "cv = CountVectorizer(max_df=0.95, min_df=2)\n",
        "dtm = cv.fit_transform(df_neg['cleaned_text'].values.astype('U'))\n",
        "dtm"
      ],
      "execution_count": 27,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "kozA0tpVQ9Y-",
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "outputId": "1e686539-10ab-4849-89b4-cfd035905a94"
      },
      "source": [
        "#run LDA model, might take some time\n",
        "from sklearn.decomposition import LatentDirichletAllocation\n",
        "LDA = LatentDirichletAllocation(n_components=10,random_state=1234)\n",
        "LDA.fit(dtm)"
      ],
      "execution_count": 28,
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
            "text/plain": [
              "LatentDirichletAllocation(batch_size=128, doc_topic_prior=None,\n",
              "                          evaluate_every=-1, learning_decay=0.7,\n",
              "                          learning_method='batch', learning_offset=10.0,\n",
              "                          max_doc_update_iter=100, max_iter=10,\n",
              "                          mean_change_tol=0.001, n_components=10, n_jobs=None,\n",
              "                          perp_tol=0.1, random_state=1234,\n",
              "                          topic_word_prior=None, total_samples=1000000.0,\n",
              "                          verbose=0)"
            ]
          },
          "metadata": {
            "tags": []
          },
          "execution_count": 28
        }
