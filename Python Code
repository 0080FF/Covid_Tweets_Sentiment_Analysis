        "id": "co6Z46fshiHw",
        "outputId": "3d2794bf-9c28-4ed7-e3b7-f304284f35bf"
      },
      "source": [
        "#add extra word to the english stopwords list\n",
        "#&amp; is the character reference for \"An ampersand\".\n",
        "cus_stopword = ['covid','covid19', '19', 'pandemic','coronavirus','corona','amp', \n",
        "                'covid_19','coronavirusoutbreak','coronavirusPandemic', '2', '1',\n",
        "                'stayathomeandstaysafe', 'coronaupdate']\n",
        "\n",
        "stopwordslist = text.ENGLISH_STOP_WORDS.union(cus_stopword) \n",
        "print(f'sklearn stopwords:  {len(stopwordslist)}')\n",
        "\n",
        "stop_words = set(stopwords.words('english')) #ex.'ve'\n",
        "print(f'NLTK stopwords:  {len(stop_words)}')\n",
        "\n",
        "stop_words.update(stopwordslist)\n",
        "\n",
        "len(stop_words)"
      ],
      "execution_count": 10,
      "outputs": [
        {
          "output_type": "stream",
