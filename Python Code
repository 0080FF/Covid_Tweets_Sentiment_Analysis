        "id": "2rk7ZpeYS_Ch"
      },
      "source": [
        "###NMF Model"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "Qs7jTQlvTArS",
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "outputId": "281ea2ea-f2ef-47ca-aec3-4b365ec4309e"
      },
      "source": [
        "#Try NMF model\n",
        "from sklearn.feature_extraction.text import TfidfVectorizer\n",
        "from sklearn.decomposition import NMF\n",
        "\n",
        "tfidf = TfidfVectorizer(max_df=0.95, min_df=2)\n",
        "dtm = tfidf.fit_transform(df_neg['cleaned_text'])\n",
        "\n",
        "nmf_model = NMF(n_components=10,random_state=42)\n",
        "\n",
        "nmf_model.fit(dtm)"
      ],
      "execution_count": 31,
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
            "text/plain": [
              "NMF(alpha=0.0, beta_loss='frobenius', init=None, l1_ratio=0.0, max_iter=200,\n",
              "    n_components=10, random_state=42, shuffle=False, solver='cd', tol=0.0001,\n",
              "    verbose=0)"
            ]
          },
          "metadata": {
            "tags": []
          },
          "execution_count": 31
        }
