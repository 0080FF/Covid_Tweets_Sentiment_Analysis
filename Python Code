      "cell_type": "markdown",
      "metadata": {
        "id": "WCgzU5uOP8u4"
      },
      "source": [
        "### Data cleaning"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "zRAqiAPNrgoc"
      },
      "source": [
        "import re\n",
        "from nltk.tokenize import RegexpTokenizer\n",
        "tokenizer = RegexpTokenizer(r'\\w+')\n",
        "\n",
        "def cleaning (text):\n",
        "\n",
        "  #remove url\n",
        "  text = re.sub(r'http\\S+','',text)\n",
        "\n",
        "  #lower case\n",
        "  text = text.lower()\n",
        "\n",
        "  #tokenize and remove puntuation\n",
        "  text = tokenizer. tokenize(text)\n",
        "\n",
        "  #remove stopwords\n",
        "  flitered = []\n",
        "  for token in text:\n",
        "    if token not in stop_words:\n",
        "      flitered.append(token)\n",
        "\n",
        "  return \" \".join(flitered)"
      ],
      "execution_count": 11,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 111
        },
        "id": "cQvn7K_dsGio",
        "outputId": "6a24947c-3ba0-4f59-b041-ac8ccfb71457"
      },
      "source": [
        "#Let's see what is it look like after cleaned\n",
        "df['cleaned_text']=df['text'].apply(cleaning)\n",
        "df.head(2)"
      ],
      "execution_count": 13,
      "outputs": [
        {
          "output_type": "execute_result",
